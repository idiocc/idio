{
  "name": "@idio/idio",
  "version": "0.2.1",
  "description": "1-Dependency Koa Web Server Compiled With Closure Compiler And Bundled With Essential Middleware: Static, Compress, Session, Cors And Router.",
  "main": "compile/index.js",
  "scripts": {
    "t": "zoroaster -a test/index.js",
    "test": "ALAMODE_ENV=test-dev yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "compile": "depack src/depack -o compile/idio.js -c -s -a -p --hide_warnings_for node_modules/@goa/router",
    "mask": "yarn t test/mask",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "lint": "eslint .",
    "doc": "doc -o README.md -n _idio -d",
    "wiki": "doc wiki -W wiki.git -n _idio -a",
    "doc-1": "doc d/static.md -o doc/static.md -n _idio -d",
    "b": "alamode src -o build -s",
    "d": "yarn-s template d1 dm do externs typework",
    "template": "typal compile/template.js -T compile/index.js -t types/api.xml",
    "dm": "typal types/modules/*.js -u",
    "do": "typal types/options/*.js -u",
    "d1": "typal types/index.js compile/index.js -u -t types",
    "externs": "typal types/externs/index.js types/externs.js",
    "build": "yarn-s d b doc",
    "typework": "typework types/config.json",
    "e": "alanode"
  },
  "files": [
    "compile",
    "types/goa",
    "types/modules/index.js",
    "types/modules/session.js",
    "types/modules/cors.js",
    "types/options/index.js"
  ],
  "alamode": true,
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/idio.git"
  },
  "typedefs": "typedefs.json",
  "keywords": [
    "idio"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/idiocc/idio/issues"
  },
  "homepage": "https://www.idio.cc",
  "devDependencies": {
    "@contexts/http": "^1.6.1",
    "@externs/goa": "^1.0.1",
    "@goa/compose": "^1.0.0",
    "@goa/cors": "^1.0.2",
    "@goa/goa": "^1.6.1",
    "@goa/http-errors": "^1.0.0",
    "@goa/koa": "^2.1.0",
    "@goa/router": "^1.2.0",
    "@goa/session": "^2.1.0",
    "@goa/statuses": "^1.0.1",
    "@goa/uuid": "^1.0.2",
    "@typedefs/goa": "^0.1.5",
    "@wrote/read": "^1.0.4",
    "alamode": "^3.3.0",
    "catchment": "^3.3.0",
    "depack": "^1.0.1",
    "documentary": "^1.35.7",
    "erotic": "^2.1.1",
    "eslint-config-artdeco": "1.0.1",
    "makepromise": "^3.1.0",
    "rqt": "^4.0.0",
    "typework": "^0.1.1",
    "yarn-s": "1.2.1",
    "zoroaster": "^4.1.2"
  },
  "dependencies": {
    "mime-db": "^1.42.0"
  }
}
