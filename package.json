{
  "name": "@idio/idio",
  "version": "0.0.0-pre",
  "description": "@Goa/Koa Web Server Bundled With Essential Middleware.",
  "main": "depack/index.js",
  "scripts": {
    "t": "zoroaster -a test/index.js",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "depack": "depack-dev src/depack -o depack -l -a -p -S -s --externs types/modules/externs.js --externs types/options/externs.js --externs types/externs.js --hide_warnings_for node_modules/util --hide_warnings_for node_modules/querystring --hide_warnings_for node_modules/http --hide_warnings_for node_modules/events",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md",
    "b": "alamode src -o build -s",
    "d": "yarn-s d1 d2 dm externs",
    "dm": "typal types/modules/*.js types/options/*.js",
    "d1": "typal types/index.js -c -t types/index.xml",
    "d2": "typal depack/index.js",
    "externs": "typal types/externs.js",
    "build": "yarn-s d b doc",
    "typework": "typework types/config.json",
    "e": "alanode"
  },
  "files": [
    "depack",
    "types/goa",
    "types/modules/index.js",
    "types/options/index.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/idio.git"
  },
  "keywords": [
    "idio"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idiocc/idio/issues"
  },
  "homepage": "https://github.com/idiocc/idio#readme",
  "devDependencies": {
    "@contexts/http": "^1.4.0",
    "@goa/goa": "^1.3.0",
    "@goa/session": "^1.0.3",
    "@goa/uuid": "^1.0.2",
    "@typedefs/goa": "^0.0.2",
    "@wrote/read": "^1.0.4",
    "alamode": "^2.3.4",
    "catchment": "^3.3.0",
    "documentary": "^1.27.3",
    "erotic": "^2.1.1",
    "eslint-config-artdeco": "1.0.1",
    "makepromise": "^3.1.0",
    "typework": "^0.1.0",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.1-alpha"
  },
  "dependencies": {
    "mime-db": "^1.40.0"
  }
}
