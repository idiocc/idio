{
  "name": "@idio/idio",
  "version": "0.2.1",
  "description": "@Goa/Koa Web Server Bundled With Essential Middleware: Static, Compress, Session, Cors.",
  "main": "compile/index.js",
  "scripts": {
    "t": "zoroaster -a test/index.js",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "compile": "depack src/depack -o compile/idio.js -c -a -p -s --hide_warnings_for node_modules/@goa/session",
    "mask": "yarn t test/mask",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "lint": "eslint .",
    "doc": "yarn-s doc-0 doc-1",
    "doc-0": "NODE_DEBUG=doc doc -o README.md -n _idio",
    "doc-1": "NODE_DEBUG=doc doc d/static.md -o doc/static.md -n _idio",
    "b": "alamode src -o build -s",
    "d": "yarn-s d1 d2 dm do externs",
    "dm": "typal types/modules/*.js -u",
    "do": "typal types/options/*.js -u",
    "d1": "typal types/index.js -c -t types/index.xml",
    "d2": "typal compile/index.js",
    "externs": "typal types/externs.js",
    "build": "yarn-s d b doc",
    "typework": "typework types/config.json",
    "e": "alanode"
  },
  "files": [
    "compile",
    "types/goa",
    "types/modules/index.js",
    "types/modules/session.js",
    "types/modules/cors.js",
    "types/options/index.js"
  ],
  "alamode": true,
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/idio.git"
  },
  "keywords": [
    "idio"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idiocc/idio/issues"
  },
  "homepage": "https://github.com/idiocc/idio#readme",
  "devDependencies": {
    "@contexts/http": "^1.5.1",
    "@externs/goa": "^0.1.1",
    "@goa/cors": "^1.0.2",
    "@goa/goa": "^1.3.6",
    "@goa/session": "^1.0.3",
    "@goa/uuid": "^1.0.2",
    "@typedefs/goa": "^0.0.6",
    "@wrote/read": "^1.0.4",
    "alamode": "^3.2.0",
    "catchment": "^3.3.0",
    "depack": "^1.0.1",
    "documentary": "^1.34.2",
    "erotic": "^2.1.1",
    "eslint-config-artdeco": "1.0.1",
    "makepromise": "^3.1.0",
    "rqt": "^4.0.0",
    "typework": "^0.1.1",
    "yarn-s": "1.2.1",
    "zoroaster": "^4.1.2"
  },
  "dependencies": {
    "mime-db": "^1.42.0"
  }
}
